# Configurações opcionais do StandVirtual Scraper
# Copie este arquivo para .env e ajuste conforme necessário

# Máximo de páginas a processar (padrão: 5)
MAX_PAGES=5

# Delay entre requisições em segundos (padrão: 1.0)
DELAY_BETWEEN_REQUESTS=1.0

# Máximo de resultados a retornar (padrão: 50)
MAX_RESULTS=50

# Timeout do Selenium em segundos (padrão: 10)
SELENIUM_TIMEOUT=10

# Executar browser em modo headless (padrão: True)
HEADLESS_BROWSER=True

# Diretório para salvar resultados (padrão: results)
RESULTS_DIR=results 